[[workflows.workflow]]
name = "DB - Backup"
author = 48391860
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üíæ Creating database backup...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"Step 1: Creating backups directory...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "mkdir -p backups"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"‚úÖ Backups directory ready\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"Step 2: Generating backup file...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "BACKUP_FILE=\"backups/backup_$(date +%Y%m%d_%H%M%S).sql\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pg_dump $DATABASE_URL > $BACKUP_FILE"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"‚úÖ Database backup created\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üìä Backup details:\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "ls -lh $BACKUP_FILE"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"üíæ Backup saved to: $BACKUP_FILE\""

